{
    "version": 2,
    "builds": [
      {
        "src": "src/server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/tipo-cambio",
        "dest": "src/server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/server/server.js"
      }
    ]
  }
  